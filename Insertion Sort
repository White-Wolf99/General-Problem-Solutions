#include<bits/stdc++.h>
#include <numeric>


using namespace std;



int main() {
  
    
    int t;
    cin>>t;
    while(t--){
        
        int n;
        cin>>n;
        int a[n];
        for(int i = 1 ; i<=n ; i++){
            cin>>a[i];
        }
        int key, j;
        for(int i = 2; i<=n ; i++){
            key = a[i]; //2
            j   = i-1;
            while(j>0 && a[j] > key){
                a[j+1] = a[j];
                j--;
            }
            a[j+1] = key;
        }

        for(int i = 1 ; i<=n ; i++){
            cout<<a[i];
        }
    
        if(t>0){
            cout<<endl;
        }

        
    }

}
